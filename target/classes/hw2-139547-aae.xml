        <name>N</name>
        <description>Different N values might be of use. To measure precision@N.</description>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>hw2-139547-evaluationAnnotator/N</parameter>
